stat1 <-get_summary_stats(datasub1, show = c("mean", "sd", "min", "max"))
kable(stat1, digits = 2) %>% kable_classic_2() %>% kable_styling(font_size = 15, )
```

## *Statistiques descriptives*

```{r}
stat2 <-get_summary_stats(datasub2, show = c("mean", "sd", "min", "max"))
kable(stat2, digits = 2) %>% kable_classic_2() %>% kable_styling(font_size = 15, )
```


```{r}
Modele0<-lm(behav1~C1GERB4_7,
            data=datasub1)

# ajout des caractéristiques individuelles : sexe, âge, éducation et état de santé
Modele1<-lm(behav1~C1GERB4_7+C1GERSEXE+C1GERAGER_1+C1GERAGER_2+C1GERAGER_3+C1GEREDU,data=datasub1)

# ajout de l'aversité face au risque et perception de la menace

Modele2<-lm(behav1~C1GERB4_7+C1GERSEXE+C1GERAGER_1+C1GERAGER_2+C1GERAGER_3+C1GEREDU+C1GERRISKGEN_1+C1GERGER5,data=datasub1)

# ajout des différents types de confiance la confiance politique (gouvernement et gouvernement fédéral) et sociale  

Modele3<-lm(behav1~C1GERB4_7+C1GERSEXE+C1GERAGER_1+C1GERAGER_2+C1GERAGER_3+C1GEREDU+C1GERRISKGEN_1+C1GERGER5+C1GERB4_0+C1GERB4_6+C1GERCONF1_2,data=datasub1)

# ajout de la satisfaction vis-à-vis de la gestion épidémique 

Modele4<-lm(behav1~C1GERB4_7+C1GERSEXE+C1GERAGER_1+C1GERAGER_2+C1GERAGER_3+C1GEREDU+C1GERRISKGEN_1+C1GERGER5+C1GERB4_0+C1GERB4_6+C1GERCONF1_2+C1GERGER4ST,data=datasub1)

# effet cumulatif
Modele5<-lm(behav1~C1GERB4_7+C1GERSEXE+C1GERAGER_1+C1GERAGER_2+C1GERAGER_3+C1GEREDU+C1GERRISKGEN_1+C1GERGER5+C1GERB4_0+C1GERB4_6+C1GERCONF1_2+C1GERGER4ST+(C1GERB4_7*C1GERRISKGEN_1),data=datasub1)

# modèle complet

Modele6<-lm(behav1~C1GERB4_7+C1GERSEXE+C1GERAGER_1+C1GERAGER_2+C1GERRISKGEN_1+C1GERGER5+C1GERB4_0+C1GERB4_6,data=datasub1)
```

## VAGUE 1 : summary du modèle complet



```{r}
summary(Modele5)
```

## VAGUE 1 : Tableau de régression 

```{r}
library(sjPlot)

tab_model(
  Modele1,Modele2,Modele3,Modele4,Modele5,Modele6,
  digits=2, 
  show.ci = FALSE, 
  show.se = TRUE,
  show.p = TRUE, 
  p.style = "stars", 
  vcov.fun = "vcovHC", 
  vcov.type = "HC1", 
  string.pred = " ", 
  string.est = "Mean (se)", 
  collapse.se = TRUE, 
  dv.labels = c("(1)","(2)","(3)","(4)","(5)","(6)"))
```

## VAGUE 1 : Validité du modèle

```{r echo=FALSE, message=FALSE, warning=FALSE}

par(mfrow=c(2,2))
plot(Modele6, 1, main = "Modele1")
plot(Modele6, 1, main = "Modele2")
plot(Modele6, 1, main = "Modele3")
plot(Modele6, 1, main = "Modele4")

library(performance)
check_model(Modele6)

library(nortest)
lillie.test(Modele6$residuals)
shapiro.test(residuals(Modele6))

library(lmtest)
bptest(Modele6)

library(car)
vif(Modele6)
```


