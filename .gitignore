# Tableau de régression : Vague 1
```{r}

Modele0<-lm(`Indice de comportement`~`Confiance dans les scientifiques`,
            data=datasub1)

# ajout des caractéristiques individuelles : sexe, âge, éducation et état de santé
Modele1<-lm(`Indice de comportement`~`Confiance dans les scientifiques`+`Sexe`+`Âge [18-34]`+`Âge [35-49]`+`Âge [50-59]`+`Education`+`Revenu [0-1250 euros]`+`Revenu [1251-3000 euros]`,data=datasub1)

# ajout de l'aversité face au risque et perception de la menace

Modele2<-lm(`Indice de comportement`~`Confiance dans les scientifiques`+`Sexe`+`Âge [18-34]`+`Âge [35-49]`+`Âge [50-59]`+`Education`+`Revenu [0-1250 euros]`+`Revenu [1251-3000 euros]`+`Aversion au risque`+`Risque de contamination`,data=datasub1)

# ajout des différents types de confiance la confiance politique (gouvernement et gouvernement fédéral) et sociale  

Modele3<-lm(`Indice de comportement`~`Confiance dans les scientifiques`+`Sexe`+`Âge [18-34]`+`Âge [35-49]`+`Âge [50-59]`+`Education`+`Revenu [0-1250 euros]`+`Revenu [1251-3000 euros]`+`Aversion au risque`+`Risque de contamination`+`Confiance dans le gouvernement du pays`+`Confiance dans le gouvernement fédéral`+`Confiance sociale`,data=datasub1)


# effet cumulatif
Modele4<-lm(`Indice de comportement`~`Confiance dans les scientifiques`+`Sexe`+`Âge [18-34]`+`Âge [35-49]`+`Âge [50-59]`+`Education`+`Revenu [0-1250 euros]`+`Revenu [1251-3000 euros]`+`Aversion au risque`+`Risque de contamination`+`Confiance dans le gouvernement du pays`+`Confiance dans le gouvernement fédéral`+`Confiance sociale`+(`Confiance dans les scientifiques`*`Aversion au risque`),data=datasub1)

# modèle complet

Modele5<-lm(`Indice de comportement`~`Confiance dans les scientifiques`+`Sexe`+`Âge [18-34]`+`Âge [35-49]`+`Revenu [0-1250 euros]`+`Revenu [1251-3000 euros]`+`Aversion au risque`+`Risque de contamination`+`Confiance dans le gouvernement du pays`+`Confiance dans le gouvernement fédéral`,data=datasub1)

library(sjPlot)

tab_model(
  Modele1,Modele2,Modele3,Modele4,Modele5,
  digits=2, 
  show.ci = FALSE, 
  show.se = TRUE,
  pred.labels = TRUE,
  show.p = TRUE, 
  p.style = "stars", 
  vcov.fun = "vcovHC", 
  vcov.type = "HC1", 
  string.pred = " ", 
  string.ci = "CI",
  string.est = "Mean (se)", 
  collapse.se = TRUE, 
  dv.labels = c("(1)","(2)","(3)","(4)","(5)","(6)"))
```

